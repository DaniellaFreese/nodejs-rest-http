{
  "name": "nodejs-rest-http",
  "version": "2.0.0",
  "author": "Red Hat, Inc.",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 6.0.0"
  },
  "scripts": {
    "test": "tape test/*.js | tap-spec",
    "test:integration": "tape test/integration/*.js | tap-spec",
    "test:integration:undeploy": "nodeshift --strictSSL=false undeploy",
    "lint": "xo",
    "prepare": "echo 'To confirm CVE compliance, run \"npm run security-check\"' ",
    "security-check": "nsp check",
    "coverage": "nyc npm test",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "ci": "npm run lint && npm run coveralls",
    "dependencyCheck": "szero . --ci",
    "release": "standard-version -a",
    "openshift": "nodeshift --strictSSL=false --nodeVersion=10.x",
    "postinstall": "license-reporter report -s && license-reporter save -s --xml licenses.xml",
    /* "start": "node ." */
  },
  "main": "./bin/www",
  "standard-version": {
    "scripts": {
      "postbump": "npm run postinstall && node release.js",
      "precommit": "git add .openshiftio/application.yaml licenses/"
    }
  },
  "xo": {
    "space": 2,
    "rules": {
      "space-before-function-paren": [
        "error",
        "always"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bucharest-gold/nodejs-rest-http.git"
  },
  "files": [
    "package.json",
    "app.js",
    "public",
    "bin",
    "LICENSE",
    "licenses"
  ],
  "bugs": {
    "url": "https://github.com/bucharest-gold/nodejs-rest-http/issues"
  },
  "homepage": "https://github.com/bucharest-gold/nodejs-rest-http",
  "devDependencies": {
    "coveralls": "^3.0.0",
    "js-yaml": "^3.10.0",
    "nodeshift": "~1.12.0",
    "nsp": "~3.2.1",
    "nyc": "~12.0.1",
    "rhoaster": "^0.2.0",
    "standard-version": "^4.3.0",
    "supertest": "^3.0.0",
    "szero": "^1.0.0",
    "tap-spec": "~5.0.0",
    "tape": "~4.9.0",
    "xo": "~0.22.0",
    "@vue/cli-plugin-babel": "3.0.0",
		"@vue/cli-plugin-e2e-nightwatch": "3.0.0",
		"@vue/cli-plugin-eslint": "3.0.0",
		"@vue/cli-plugin-pwa": "3.0.0",
		"@vue/cli-plugin-unit-jest": "3.0.0",
		"@vue/cli-service": "3.0.0",
		"@vue/eslint-config-prettier": "3.0.0",
		"@vue/test-utils": "1.0.0-beta.20",
		"babel-core": "6.26.3",
		"babel-jest": "22.4.3",
		"babel-loader": "7.1.5",
		"babel-plugin-transform-es2015-shorthand-properties": "6.24.1",
		"babel-plugin-transform-imports": "1.5.0",
		"babel-preset-env": "1.7.0",
		"babel-preset-react": "6.24.1",
		"eslint-plugin-graphql": "1.5.0",
		"graphql-tag": "2.9.2",
		"jest-serializer-vue": "1.0.0",
		"jest-transform-graphql": "2.1.0",
		"lint-staged": "6.1.1",
		"node-sass": "4.9.0",
		"sass-loader": "7.0.1",
		"vue-cli-plugin-apollo": "0.8.3",
		"vue-jest": "2.6.0",
		"vue-loader": "14.2.2",
		"vue-server-renderer": "2.5.16",
		"vue-styleguidist": "1.7.8",
		"vue-template-compiler": "2.5.16"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "debug": "^3.1.0",
    "express": "4.16.0",
    "license-reporter": "^1.2.0",
    "apollo-cache-inmemory": "1.2.1",
    "apollo-client": "2.3.1",
    "apollo-link": "1.2.2",
    "apollo-link-context": "1.0.8",
    "apollo-link-http": "1.5.4",
    "apollo-link-persisted-queries": "0.2.0",
    "apollo-link-ws": "1.0.8",
    "apollo-upload-client": "8.0.0",
    "apollo-utilities": "1.0.12",
    "axios": "0.18.0",
		"babel-polyfill": "6.26.0",
    "d3": "5.4.0",
		"express": "4.16.3",
    "graphql": "0.13.2",
		"luxon": "1.2.1",
		"register-service-worker": "1.3.0",
    "subscriptions-transport-ws": "0.9.9",
		"unfetch": "3.0.0",
    "vue": "2.5.16",
		"vue-apollo": "3.0.0-beta.16",
		"vue-router": "3.0.1",
		"vue2-filters": "0.3.0",
		"vuetify": "1.1.4",
		"vuex": "3.0.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "uglify": true
          },
          "modules": false
        }
      ],
      "react"
    ],
    "plugins": [
      [
        "transform-imports",
        {
          "vuetify": {
            "transform": "vuetify/es5/components/${member}",
            "preventFullImport": false
          },
          "bcbsnc-platform": {
            "transform": "bcbsnc-platform/src/components/${member}",
            "preventFullImport": true
          }
        }
      ],
      "transform-es2015-shorthand-properties",
      "transform-class-properties",
      "transform-object-rest-spread",
      "transform-runtime"
    ],
    "env": {
      "test": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": 8
              }
            }
          ]
        ],
        "plugins": [
          "transform-class-properties",
          "transform-object-rest-spread"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "env": {
      "browser": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "semi-style": 1,
      "semi": 2,
      "no-console": 1,
      "no-debugger": 1
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "useTabs": true
  },
  "eslintIgnore": [
    "/build/",
    "/dist/",
    "/*.js",
    "/test/unit/coverage/"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie 11"
  ],
  "jest": {
    "testURL": "http://localhost/",
    "transformIgnorePatterns": [
      "node_modules/(?!bcbsnc-platform)"
    ],
    "testMatch": [
      "<rootDir>/(tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx))"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "coverageDirectory": "<rootDir>/tests/unit/coverage",
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!src/modules/**",
      "!src/registerServiceWorker.js",
      "!src/components/ExampleHeading.js",
      "!src/components/ExampleLinksList.vue",
      "!src/main.js",
      "!src/router/index.js",
      "!**/node_modules/**"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint --no-ignore",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint --no-ignore",
      "git add"
    ]
  }
}
